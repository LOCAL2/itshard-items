# Production Environment Variables
# สำหรับ production deployment

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_anon_key_here

# Discord OAuth Configuration (Production)
VITE_DISCORD_CLIENT_ID=your_production_discord_client_id
VITE_DISCORD_CLIENT_SECRET=your_production_discord_client_secret
VITE_DISCORD_REDIRECT_URI=https://yourdomain.com/auth/discord/callback

# Discord Webhook (Production)
VITE_DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_production_webhook
VITE_DISCORD_THREAD_ID=your_production_thread_id
VITE_DISCORD_EMBED_COLOR=0x5865F2

# Application Configuration
VITE_APP_NAME=ระบบจัดการครอบครัว
VITE_APP_VERSION=1.0.0
VITE_DEV_MODE=false
VITE_DEBUG_MODE=false